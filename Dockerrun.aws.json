{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "web",
      "image": "dockerquark/journal-web-prod",
      "hostname": "web",
      "essential": false,
      "memory": 128
    },
    {
      "name": "api",
      "image": "dockerquark/journal-api-prod",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "dockerquark/journal-nginx-prod",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client", "api"]
    }
  ]
}
